{
  "expo": {
    "name": "salty-solutions-mobile-v2b",
    "slug": "salty-solutions-mobile-v2b",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "primaryColor": "#4a5568",
    "scheme": "saltysolutionsmobilev2b",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.musumeche.salty.solutions"
    },
    "android": {
      "package": "com.musumeche.salty.solutions",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#4a5568"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "29c6d436-f00e-4bed-a7e7-6d501cd7df3a"
      }
    },
    "owner": "stevenmusumeche"
  }
}
